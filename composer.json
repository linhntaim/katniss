{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories":
    [
        {
            "type": "vcs",
            "url": "https://github.com/linhntaim/ortc-laravel"
        },
        {
            "type": "vcs",
            "url": "https://github.com/linhntaim/ortc-php"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "barryvdh/laravel-debugbar": "^3.1",
        "barryvdh/laravel-elfinder": "^0.4.0",
        "barryvdh/laravel-snappy": "^0.4.1",
        "chumper/zipper": "^1.0",
        "dimsav/laravel-translatable": "^9.1",
        "fideloper/proxy": "^4.0",
        "h4cc/wkhtmltoimage-amd64": "^0.12.4",
        "h4cc/wkhtmltoimage-i386": "^0.12.4",
        "h4cc/wkhtmltopdf-amd64": "^0.12.4",
        "h4cc/wkhtmltopdf-i386": "^0.12.4",
        "intervention/image": "^2.4",
        "jenssegers/agent": "^2.6",
        "larabros/elogram": "^1.2",
        "laravel/framework": "5.6.*",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "^1.0",
        "maatwebsite/excel": "^3.0",
        "mcamara/laravel-localization": "^1.3",
        "mews/purifier": "^2.1",
        "moontoast/math": "^1.1",
        "thunderer/shortcode": "^0.6.5",
        "wemersonjanuario/wkhtmltopdf-windows": "0.12.2.3",
        "zizaco/entrust": "^1.9"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "Katniss\\": "app/"
        },
        "files": [
            "app/Everdeen/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
